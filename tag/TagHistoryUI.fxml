<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<VBox prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pack.tag.TagHistoryUIController">
    <children>
        <ListView fx:id="tagHistory" editable="true" prefHeight="308.0" prefWidth="300.0"/>
        <ButtonBar prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button fx:id="revert" mnemonicParsing="false" onAction="#handleRevert" prefHeight="20.0"
                        prefWidth="70.0" text="Revert"/>
            </buttons>
        </ButtonBar>
    </children>
</VBox>
